<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<!-- 控制台输出 -->
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger.%line - %msg%n</pattern>
		</encoder>
	</appender>
	 
	<root level="INFO">
		<appender-ref ref="stdout" />
	</root>
</configuration>
